#:schema node_modules/wrangler/config-schema.json
name = "api"
main = "src/index.js"
compatibility_date = "2024-05-04"
compatibility_flags = ["nodejs_compat"]

# 自定义域名
routes = [
  { pattern = "example.com", custom_domain = true },
]

workers_dev = true

placement = { mode = "smart" }

[vars]
# 企业微信机器人webhook地址
QYWXW = ""
